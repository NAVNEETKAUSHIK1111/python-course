num1 = float(input("Enter first number: "))
num2 = float(input("Enter second number: "))
sum = num1 + num2
sub = num1 - num2
mul = num1 * num2
div = num1 / num2
print("The sum of", num1, "and", num2, "is", sum)
print("The sub of", num1, "and", num2, "is", sum)
print("The mul of", num1, "and", num2, "is", sum)
print("The div of", num1, "and", num2, "is", sum)
